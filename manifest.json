{
	"manifest_version": 2,

	"name": "Bàb smile +",

	"version": "4.22.9.27",

	"description": "Débloque la totalité des smileys sur Brick à Brack !",

	"author": "alberic89",


	"icons": {
		"16": "icons/logo16.png",
		"32": "icons/logo32.png",
		"48": "icons/logo48.png",
		"96": "icons/logo96.png",
		"128": "icons/logo128.png"
	},

	"content_scripts": [
		{
			"matches": ["*://brick-a-brack.com/*"],
			"js": ["main.js"]
		}
	],
    "permissions": [
		"*://raw.githubusercontent.com/alberic89/B-b-smile--/main/ressources/*",
		"webRequest"
	]
}