{
    "manifest_version": 3,

    "name": "Bàb Smile +",

    "version": "5.1.1",

    "description": "Débloque la totalité des smileys sur Brickfilms.com (Brick à Brack) !",

    "author": "alberic89",

    "icons": {
        "16": "icons/logo16.png",
        "32": "icons/logo32.png",
        "48": "icons/logo48.png",
        "96": "icons/logo96.png",
        "128": "icons/logo128.png"
    },

    "content_scripts": [
        {
            "matches": ["*://brickfilms.com/*"],
            "js": ["main.js"]

        }
    ],
    "permissions": [
        "webRequest"
    ]
}
